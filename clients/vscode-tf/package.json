{
  "name": "@tf-lang/vscode-tf",
  "displayName": "TF Language Tools",
  "version": "0.1.0",
  "publisher": "tf-lang",
  "type": "module",
  "main": "dist/extension.js",
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:tf"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tf",
        "aliases": ["TF"],
        "extensions": [".tf"]
      }
    ],
    "grammars": [
      {
        "language": "tf",
        "scopeName": "source.tf",
        "path": "./syntaxes/tf.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.9.2"
  }
}
