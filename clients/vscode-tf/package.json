{
  "name": "vscode-tf",
  "displayName": "TF Language Tools",
  "version": "0.1.0",
  "private": true,
  "publisher": "tf-lang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "dist/extension.js",
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "contributes": {
    "languages": [
      {
        "id": "tf",
        "aliases": ["TF"],
        "extensions": [".tf"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tf",
        "scopeName": "source.tf",
        "path": "./syntaxes/tf.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
