{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OracleReport",
  "type": "object",
  "required": [
    "version",
    "ok"
  ],
  "properties": {
    "version": {
      "type": "string"
    },
    "ok": {
      "type": "boolean"
    },
    "violations": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "mismatches": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "diff": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": true
}