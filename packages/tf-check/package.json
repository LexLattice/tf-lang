{
  "name": "@tf-lang/tf-check",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "tf-check": "dist/cli.js"
  },
